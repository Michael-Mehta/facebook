<% if user_signed_in? %>

<h1> Discover Friends </h1>
<br>
<br>
<% @users.each do |user|%>
    
        <% if current_user.following?(user) || current_user == user%>
         <% else %>
         <%= image_tag avatar_url_for(user,32), class: "rounded-circle m-4" %>
         <strong>@<%= user.username %></strong>
         <%= button_to "Friend", follow_path(user) %>
         <br>
         <% end %>

<% end %>
<% end %>